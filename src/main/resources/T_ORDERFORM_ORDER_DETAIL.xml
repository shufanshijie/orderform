<?xml version="1.0" encoding="UTF-8"?>
<Haiyan xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../haiyan.xsd">
	<table name="T_ORDERFORM_ORDER_DETAIL" description="订单明细信息">
		<id name="ID" javaType="string" uiname="ID" description="订单明细Id">
			<component type="hidden" url="#"/>
			<queryCondition type="equal"/>
		</id>
		<field name="ORDERID" uiname="ORDERID" description="订单id" javaType="string" >
			<component type="text"/>
			<queryCondition type="equal"/>
		</field>
		<field name="IMAGE" uiname="IMAGE" description="套餐图片" javaType="string" nullAllowed="false" length="256" defaultValue="http://images.5proapp.com/noportrait">
			<component type="text"/>
			<queryCondition type="equal"/>
		</field>
		<field name="NAME" uiname="NAME" description="套餐名称" javaType="string" >
			<component type="text"/>
			<queryCondition type="equal"/>
		</field>
		<field name="COUNT" uiname="COUNT" description="订购数量" javaType="integer" >
			<component type="integer"/>
			<queryCondition type="equal"/>
		</field>
		<field name="DISPATCHINGADDRESS" uiname="DISPATCHINGADDRESS" description="订单配送地址" javaType="string">
			<component type="text"/>
		</field>
		<field name="DISPATCHINGDATE" uiname="DISPATCHINGDATE" description="送货时间" javaType="date" nullAllowed="true" dataStyle="yyyy-MM-dd">
			<component type="date" colspan="3" readOnly="true"/>
			<queryCondition type="region"/>
		</field>
		<field name="DISPATCHINGTYPE" uiname="DISPATCHINGTYPE" description="配送方式" javaType="string" nullAllowed="false" defaultValue="0">
			<option displayName="自提" value="0"/>
			<option displayName="送货上门" value="1"/>
			<component type="dropdown" />
			<queryCondition type="equal"/>
		</field>
		<field name="PRICE" uiname="PRICE" description="套餐单价" javaType="bigDecimal" >
			<component type="bigDecimal"/>
			<queryCondition type="equal"/>
		</field>
		<field name="TOTALPRICE" uiname="TOTALPRICE" description="价格" javaType="bigDecimal" >
			<component type="bigDecimal"/>
			<queryCondition type="equal"/>
		</field>
		<queryFilter>
			<pluggedFilter><![CDATA[
				order by t_1.DISPATCHINGDATE
			]]></pluggedFilter>
		</queryFilter>
	</table>
</Haiyan>
